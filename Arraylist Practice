// This program sets up an Array, adds a value at a specfic location, and then returns the velue in each spot in order. 

import java.util.ArrayList;

public class TemperaturesC {
	
	public static void main(String[] args) {

		ArrayList<Integer> weeklyTemperatures = new ArrayList<Integer>();
	// Sets up the list
		weeklyTemperatures.add(78);
		weeklyTemperatures.add(67);
		weeklyTemperatures.add(89);
		weeklyTemperatures.add(94);
	// Inserts a value at a specfic location
    		weeklyTemperatures.add(2, 111);
	// a For loop that prints out the value in each spot in the list
    for (int j = 0; j < weeklyTemperatures.size(); j++){
      
      System.out.println( weeklyTemperatures.get(j) );
      
    }
		

	}
}
